<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.cs361.se15.wellliv">

<!-- To auto-complete the email text field in the login form with the user's emails -->
<uses-permission android:name="android.permission.GET_ACCOUNTS" />
<uses-permission android:name="android.permission.READ_PROFILE" />
<uses-permission android:name="android.permission.READ_CONTACTS" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher_test"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_test_round"
    android:supportsRtl="true"
    android:theme="@style/WellLiveTheme">
    <activity
        android:name=".LoginActivity"
        android:label="@string/app_name"
        android:theme="@style/WellLiveTheme">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:logo="@mipmap/ic_launcher_foreground_test"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name=".PTSDActivity"
        android:label="@string/title_ptsd"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name=".SymptomLogActivity"
        android:label="@string/title_symptoms"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name=".AbuseActivity"
        android:label="@string/title_abuse"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name=".DepressionActivity"
        android:label="@string/title_depression"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name=".SexualAssaultActivity"
        android:label="@string/title_assault"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name=".AddSymptomActivity"
        android:label="@string/title_add_symptom"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name=".SettingsActivity"
        android:label="@string/title_settings"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name="com.cs361.se15.wellliv.HotlineActivity"
        android:label="@string/title_hotlines"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name="com.cs361.se15.wellliv.PsytherapyActivity"
        android:label="@string/title_psychotherapy"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    <activity
        android:name="com.cs361.se15.wellliv.SupportActivity"
        android:label="@string/title_supportgroups"
        android:parentActivityName=".MainActivity"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
</application>

</manifest>